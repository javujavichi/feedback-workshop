{
  "name": "feedback-workshop (Java 11 - custom)",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "forwardPorts": [8080],
  "postCreateCommand": "chmod +x ./gradlew || true && ./gradlew --version",
  "extensions": [
    "vscjava.vscode-java-pack",
    "vscjava.vscode-spring-initializr",
    "pivotal.vscode-boot-dev-pack",
    "richardwillis.vscode-gradle"
  ],
  
  "remoteUser": "vscode"
}